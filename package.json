{
  "name": "fullstack-project",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://abhigaprasad.github.io/fullstack-project",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d frontend/build",
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start-backend": "cd backend && npm install && npm start",
    "start-frontend": "cd frontend && npm install && npm start",
    "install-backend": "cd backend && npm install",
    "install-frontend": "cd frontend && npm install",
    "build": "cd frontend && npm run build && cd ../backend && npm run build",
    "vercel-build": "npm install && npm run build",
    "dev": "concurrently \"npm run dev-backend\" \"npm run dev-frontend\"",
    "dev-backend": "cd backend && npm run dev",
    "dev-frontend": "cd frontend && npm start"
  },
  "dependencies": {
    "concurrently": "^7.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "^5.0.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "nodemon": "^3.1.9"
  },
  "engines": {
    "node": "22.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
